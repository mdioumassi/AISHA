<?php

namespace InscriptionBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * MensualiteNiveau
 *
 * @ORM\Table(name="mensualite_niveau")
 * @ORM\Entity(repositoryClass="InscriptionBundle\Repository\MensualiteNiveauRepository")
 */
class MensualiteNiveau
{
    /**
     * @var int
     *
     * @ORM\Column(name="id", type="integer")
     * @ORM\Id
     * @ORM\GeneratedValue(strategy="AUTO")
     */
    private $id;

    /**
     * @var bool
     *
     * @ORM\Column(name="paye", type="boolean")
     */
    private $paye;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="created_at", type="datetime")
     */
    private $createdAt;

    /**
     * @var \DateTime
     *
     * @ORM\Column(name="updated_at", type="datetime")
     */
    private $updatedAt;

    /**
     * @ORM\ManyToOne(targetEntity="InscriptionBundle\Entity\Mensualite")
     */
    private $mensualite;

    /**
     * @ORM\ManyToOne(targetEntity="InscriptionBundle\Entity\Niveau")
     */
    private $niveau;
}

